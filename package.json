{
  "name": "snet-sdk-core",
  "version": "0.1.1",
  "description": "SingularityNET core SDK for JS",
  "keywords": [
    "snet-sdk",
    "singularitynet-sdk",
    "singularitynet"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/singnet/snet-sdk-core.git"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/*",
    "README.md"
  ],
  "scripts": {
    "build": "babel src --out-dir dist",
    "clean": "rimraf dist",
    "docGen": "jsdoc2md src/*.js > ./docs/docs.md",
    "lint": "eslint src",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "prepublishOnly": "npm run lint && npm run clean && npm run build"
  },
  "author": "SingularityNET Foundation",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/singnet/snet-sdk-core/issues"
  },
  "homepage": "https://github.com/singnet/snet-sdk-core#readme",
  "dependencies": {
    "bignumber.js": "9.1.2",
    "loglevel": "1.9.2",
    "singularitynet-platform-contracts": "2.1.0",
    "singularitynet-token-contracts": "3.1.0"
  },
  "exports": {
    "./constants/*": "./dist/src/constants/*.js",
    "./mpe/*": "./dist/src/mpe/*.js",
    "./paymentStrategies/*": "./dist/src/paymentStrategies/*.js",
    "./training/*": "./dist/src/training/*.js",
    "./utils/*": "./dist/src/utils/*.js"
  },
  "peerDependencies": {
    "lodash": "4.17.21",
    "web3": "4.16.0"
  },
  "devDependencies": {
    "@babel/cli": "7.26.4",
    "@babel/core": "7.26.9",
    "@babel/node": "7.26.0",
    "@babel/preset-env": "7.26.9",
    "@babel/register": "7.25.9",
    "babel-eslint": "10.1.0",
    "babel-plugin-module-resolver": "5.0.2",
    "eslint": "9.21.0",
    "jest": "29.7.0",
    "jsdoc-to-markdown": "9.1.1"
  }
}
