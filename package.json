{
  "name": "test-snet-sdk-core",
  "version": "0.1.0",
  "description": "SingularityNET core SDK for JS",
  "keywords": [
    "snet-sdk",
    "singularitynet-sdk",
    "singularitynet"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/singnet/snet-sdk-core.git"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/*",
    "README.md"
  ],
  "scripts": {
    "build": "babel src --out-dir dist",
    "clean": "rimraf dist",
    "docGen": "jsdoc2md src/*.js > ./docs/docs.md",
    "lint": "eslint src",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "prepublishOnly": "npm run lint && npm run clean && npm run build"
  },
  "author": "SingularityNET Foundation",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/singnet/snet-sdk-core/issues"
  },
  "homepage": "https://github.com/singnet/snet-sdk-core#readme",
  "dependencies": {
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.2",
    "bignumber.js": "^9.1.2",
    "loglevel": "^1.9.2",
    "singularitynet-platform-contracts": "^1.0.4",
    "singularitynet-token-contracts": "^3.0.3"
  },
  "peerDependencies": {
    "lodash": "^4.17.21",
    "web3": "^4.16.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/node": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/register": "^7.23.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "jsdoc-to-markdown": "^9.0.4",
    "rollup": "^4.24.3",
    "rollup-plugin-peer-deps-external": "^2.2.4"
  }
}
